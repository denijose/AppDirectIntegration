<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.denis.main.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
		<servlet>
	    <servlet-name>AppDirectIntegrationServlet</servlet-name>
	    <servlet-class>com.denis.main.AppDirectIntegrationServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>AppDirectIntegrationServlet</servlet-name>
	    <url-pattern>/appdirect</url-pattern>
	</servlet-mapping>
	
	<servlet>
	    <servlet-name>PullDataServlet</servlet-name>
	    <servlet-class>com.denis.main.PullDataServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	    <servlet-name>PullDataServlet</servlet-name>
	    <url-pattern>/pull</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
